<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mcgroup16.asu.com.group16">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="mcgroup16.asu.com.group16.activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mcgroup16.asu.com.group16.activity.GraphActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="mcgroup16.asu.com.group16.activity.DataCollectActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>